void printText(void) {

String s;

if (Serial.available()) {

s = Serial.readString(); //getting string input in variable "s"

if (s.substring(0,8) == "Messages") {
display.drawBitmap(10, -5,  SMS, 128, 64, 1);
 display.display();
  delay(1000);
  display.clearDisplay();
}
if (s.substring(0,8) == "WhatsApp") {
display.drawBitmap(10, -5,  Whatsapp, 128, 64, 1);
 display.display();
  delay(1000);
  display.clearDisplay();
}

if (s.substring(0,10) == "MapmyIndia") {
display.drawBitmap(10, -5,  Navi, 128, 64, 1);
 display.display();
  delay(1000);
  display.clearDisplay();
}

if (s.substring(0,13) == "Incoming Call") {
display.drawBitmap(10, -5,  Call, 128, 64, 1);
 display.display();
  delay(1000);
  display.clearDisplay();
}

if (s.substring(0,5) == "Gmail") {
display.drawBitmap(10, -5,  Mail, 128, 64, 1);
 display.display();
  delay(1000);
  display.clearDisplay();
}


display.println(s);

display.display();

display.println("\n");

}

delay(7000);

}

void testdrawtriangle(void) {
  for (int16_t i=0; i<min(display.width(),display.height())/3; i+=1) {
    display.drawTriangle(display.width()/2, display.height()/2-i,
                     display.width()/2-i, display.height()/2+i,
                     display.width()/2+i, display.height()/2+i, WHITE);
    display.display();
    delay(1);
  }
}

